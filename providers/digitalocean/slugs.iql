--sizes
SELECT
slug,
description as 'class',
memory as 'ram ',
vcpus as 'cpu',
disk,
transfer,
price_monthly,
price_hourly
FROM digitalocean.sizes.sizes;

--distro images 
SELECT
id,
slug,
type,
distribution as 'distro',
status,
min_disk_size,
size_gigabytes
FROM
digitalocean.images.images;

